using System;

class Program
{
    static void Main()
    {
        Console.Write("Enter base: ");
        int b = int.Parse(Console.ReadLine());
        Console.Write("Enter exponent: ");
        int e = int.Parse(Console.ReadLine());

        int result = 1;
        for (int i = 0; i < e; i++)
            result *= b;

        Console.WriteLine($"Result: {result}");
    }
}
